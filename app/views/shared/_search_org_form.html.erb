<div class="row">
<%= form_tag("/searchorg", method: "get") do %>
	<span class="span2">
	  <%= label_tag(:name, "Search by name:") %>
	</span>
	<span class="span4">
	  <%= text_field_tag(:name, @name) %>
	</span>
	<span class="span4">
	  <%= submit_tag("Search", class: 'btn') %>
	  <%= link_to "Create new organization", createorg_path, :class => "movedown signup_button round"  %>
	</span>
<% end %>
</div>