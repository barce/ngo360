<h1>Edit Profile</h1>

<div class="row">
  <div class="span6 offset3">
<%= form_for(@organization) do |f| %>
  <%= render 'shared/error_messages', :object => f.object %>
  <%= render 'fields', :f => f %>
  <div class="actions">
    <%= f.submit "Update", class: 'btn' %>
  </div>
<% end %>
  </div>
</div>
